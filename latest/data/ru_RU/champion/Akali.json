ссенций тени","Перезарядка"],"effect":["{{ e1 }} -> {{ e1NL }}","{{ f1 }} -> {{ f2 }}","{{ cooldown }} -> {{ cooldownnNL }}"]},"maxrank":3,"cooldown":[2,1.5,1],"cooldownBurn":"2/1.5/1","cost":[0,0,0],"costBurn":"0","effect":[null,[100,175,250],[2000,2000,2000],[30,22.5,15],[3,3,3],[1,1,1],[100,100,100],[200,200,200],[600,600,600],[225,225,225]],"effectBurn":[null,"100/175/250","2000","30/22.5/15","3","1","100","200","600","225"],"vars":[{"link":"spelldamage","coeff":0.5,"key":"a1"},{"link":"@cooldownchampion","coeff":[30.0,22.5,15.0],"key":"f1"},{"link":"@cooldownchampion","coeff":[22.5,15.0],"key":"f2"}],"costType":"EssenceofShadow","maxammo":"3","range":[700,700,700],"rangeBurn":"700","image":{"full":"AkaliShadowDance.png","sprite":"spell0.png","group":"spell","x":240,"y":96,"w":48,"h":48},"resource":"{{ e5 }} Эссенция тени"}],"passive":{"name":"Параллельные дисциплины","description":"Дисциплина силы: автоатаки Акали наносят дополнительный магический урон, равный 6% от силы атаки (плюс 1% от силы атаки за каждые 6 единиц силы умений).<br><br>Дисциплина мощи: магический вампиризм Акали увеличен на 6% (плюс 1% за каждые 6 единиц дополнительной силы атаки).","image":{"full":"AkaliTwinDisciplines.png","sprite":"passive0.png","group":"passive","x":96,"y":0,"w":48,"h":48}},"recommended":[{"champion":"Akali","title":"AkaliCS","map":"CS","mode":"ODIN","type":"riot","customTag":"","extensionPage":false,"customPanel":null,"blocks":[{"type":"starting","recMath":false,"recSteps":false,"minSummonerLevel":-1,"maxSummonerLevel":-1,"showIfSummonerSpell":"","hideIfSummonerSpell":"","items":[{"id":"1029","count":1,"hideCount":false},{"id":"2003","count":4,"hideCount":false},{"id":"3340","count":1,"hideCount":false}]},{"type":"early","recMath":false,"recSteps":false,"minSummonerLevel":-1,"maxSummonerLevel":-1,"showIfSummonerSpell":"","hideIfSummonerSpell":"","items":[{"id":"1001","count":1,"hideCount":false},{"id":"3145","count":1,"hideCount":false},{"id":"3144","count":1,"hideCount