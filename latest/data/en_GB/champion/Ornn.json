ttle targets.</rules>","leveltip":{"label":["Maximum Damage","Minimum Damage","@AbilityResourceName@ Cost"],"effect":["{{ e0 }}% -> {{ e0NL }}%","{{ e3 }} -> {{ e3NL }}","{{ cost }} -> {{ costNL }}"]},"maxrank":5,"cooldown":[12,12,12,12,12],"cooldownBurn":"12","cost":[45,50,55,60,65],"costBurn":"45/50/55/60/65","datavalues":{},"effect":[null,[0.75,0.75,0.75,0.75,0.75],[3,3,3,3,3],[80,130,180,230,280],[0.024,0.026,0.028,0.03,0.032],[3.5,3.5,3.5,3.5,3.5],[175,175,175,175,175],[500,500,500,500,500],[0.35,0.35,0.35,0.35,0.35],[7,7,7,7,7],[12,13,14,15,16]],"effectBurn":[null,"0.75","3","80/130/180/230/280","0.024/0.026/0.028/0.03/0.032","3.5","175","500","0.35","7","12/13/14/15/16"],"vars":[],"costType":" {{ abilityresourcename }}","maxammo":"-1","range":[25000,25000,25000,25000,25000],"rangeBurn":"25000","image":{"full":"OrnnW.png","sprite":"spell8.png","group":"spell","x":384,"y":144,"w":48,"h":48},"resource":"{{ cost }} {{ abilityresourcename }}"},{"id":"OrnnE","name":"Searing Charge","description":"Ornn charges, dealing damage to enemies he passes through. If Ornn collides with terrain while charging, the impact creates a shockwave around him which deals damage and knocks up enemies.","tooltip":"Ornn charges, dealing {{ e1 }} <scaleArmor>(+{{ f2 }})</scaleArmor><span class=\"colorFF00FF\">(+{{ f3 }})</span> physical damage to enemies hit.<br /><br />If Ornn rams into terrain, he creates a shockwave that knocks up enemies for {{ e6 }} second and applies Searing Charge's damage to those not hit by the charge.<br /><br /><rules>The shockwave destroys magma pillars and terrain created by enemy champions.</rules>","leveltip":{"label":["Dash Damage","Shockwave Damage","Cooldown","@AbilityResourceName@ Cost"],"effect":["{{ e1 }} -> {{ e1NL }}","{{ e2 }} -> {{ e2NL }}","{{ cooldown }} -> {{ cooldownNL }}","{{ cost }} -> {{ costNL }}"]},"maxrank":5,"cooldown":[16,15,14,13,12],"cooldownBurn":"16/15/14/13/12","cost":[35,40,45,50,55],"costBurn":"35/40/45/50/55","datavalues":{},"