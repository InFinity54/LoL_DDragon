t":[0,0,0],"costBurn":"0","datavalues":{},"effect":[null,[150,250,350],[1.1,1.1,1.1],[700,850,1000],[-0.3,-0.4,-0.5],[2.5,2.5,2.5],[275,275,275],[300,300,300],[1,1,1],[0.25,0.25,0.25],[0.1,0.1,0.1]],"effectBurn":[null,"150/250/350","1.1","700/850/1000","-0.3/-0.4/-0.5","2.5","275","300","1","0.25","0.1"],"vars":[],"costType":"无消耗","maxammo":"-1","range":[300,300,300],"rangeBurn":"300","image":{"full":"ZacR.png","sprite":"spell15.png","group":"spell","x":336,"y":96,"w":48,"h":48},"resource":"无消耗"}],"passive":{"name":"组织分裂","description":"当札克施放技能击中敌人时，本体部份的组织会自动脱落。如果把散落的组织重新回收的话，可以回复生命。遭到击杀时，札克会分裂成四块软泥，缓速向中央集中。如果任一软泥存活，他可以立即于原地复活，复活后的生命依残存软泥的生命而定。每一块软泥保有札克特定比例的最大生命、 物理防御和魔法防御，此技能有 5 分钟的冷却时间。","image":{"full":"ZacPassive.png","sprite":"passive5.png","group":"passive","x":96,"y":0,"w":48,"h":48}},"recommended":[]}}}