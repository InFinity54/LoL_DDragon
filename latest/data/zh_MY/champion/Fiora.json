aling>{{ healpersecondcalc }} 生命</healing>，持续 {{ minhealduration }} 至 {{ healduration }} 秒，根据破绽击中的数量增加。","leveltip":{"label":["冷却时间","不屈斗魂移动速度","每秒治愈生命"],"effect":["{{ cooldown }}->{{ cooldownNL }}","{{ e2 }}%->{{ e2NL }}%","{{ e7 }}->{{ e7NL }}"]},"maxrank":3,"cooldown":[110,90,70],"cooldownBurn":"110/90/70","cost":[100,100,100],"costBurn":"100","datavalues":{},"effect":[null,[8,8,8],[30,40,50],[0,0,0],[30,45,60],[500,500,500],[5,5,5],[80,110,140],[550,550,550],[2,2,2],[1,1,1]],"effectBurn":[null,"8","30/40/50","0","30/45/60","500","5","80/110/140","550","2","1"],"vars":[],"costType":" {{ abilityresourcename }}","maxammo":"-1","range":[500,500,500],"rangeBurn":"500","image":{"full":"FioraR.png","sprite":"spell3.png","group":"spell","x":432,"y":48,"w":48,"h":48},"resource":"{{ cost }} {{ abilityresourcename }}"}],"passive":{"name":"不屈斗魂","description":"菲欧拉对附近一名敌方英雄发起挑战。她决定一个方向进行攻击。如果完成挑战，她会得到小量的奖励，并决定下一个方向。","image":{"full":"Fiora_P.png","sprite":"passive1.png","group":"passive","x":0,"y":0,"w":48,"h":48}},"recommended":[]}}}