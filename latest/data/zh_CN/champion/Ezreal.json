":[25000,25000,25000],"rangeBurn":"25000","image":{"full":"EzrealR.png","sprite":"spell3.png","group":"spell","x":48,"y":48,"w":48,"h":48},"resource":"{{ cost }} {{ abilityresourcename }}"}],"passive":{"name":"咒能高涨","description":"伊泽瑞尔的任何技能在击中目标后，都会提升他的攻击速度（最多可叠加5次）。","image":{"full":"Ezreal_RisingSpellForce.png","sprite":"passive0.png","group":"passive","x":384,"y":96,"w":48,"h":48}},"recommended":[]}}}